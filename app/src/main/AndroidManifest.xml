<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yollpoll.myframework">

    <uses-permission android:name="android.permission.CAMERA"/>
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".ui.aidldemo.ADILTestService"
            android:process=":remote"
            />
        <activity android:name=".ui.aidldemo.AIDLTestActivity"/>
        <activity android:name=".ui.databinding.DataBindingTestActivity"/>
        <activity android:name=".ui.lifecycledemo.LifecycleActivity"/>
        <activity android:name=".ui.livedatademo.LiveDataActivity"/>
        <activity android:name=".ui.pagingdemo.PagingDemoActivity"/>
    </application>

</manifest>